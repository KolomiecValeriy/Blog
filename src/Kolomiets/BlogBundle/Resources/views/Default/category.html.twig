{% extends "KolomietsBlogBundle:Default:template.html.twig" %}

{% block head %}{% trans %}New category{% endtrans %}{% endblock %}
{% block text %}
    {{ form_start(form) }}
    <div class="form-group">
        {{ form_row(form.name, { 'attr': {'class': 'form-control'} }) }}
    </div>
    <div class="form-group">
        {{ form_row(form.save, { 'attr': {'class': 'btn btn-default'} }) }}
    </div>
    {{ form_end(form) }}
{% endblock %}
